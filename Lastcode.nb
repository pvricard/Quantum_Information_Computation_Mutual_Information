 RowBox[{
  RowBox[{
   RowBox[{"eigen1", "[", "\[Theta]_", "]"}], ":=", 
   RowBox[{"Select", "[", 
    RowBox[{
     RowBox[{"Eigenvalues", "[", 
      RowBox[{"\[Rho]B3", "[", "\[Theta]", "]"}], "]"}], ",", 
     RowBox[{
      RowBox[{"#", ">", 
       RowBox[{"10", "^", 
        RowBox[{"(", 
         RowBox[{"-", "10"}], ")"}]}]}], "&"}]}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.8792464311858177`*^9, 3.8792464708643913`*^9}, {
