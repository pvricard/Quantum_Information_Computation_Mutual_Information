Cell["\<\
Then let us define the \[Rho]_{y|x} through its eigenvalues, where each array \
accounts for \[Rho]_{y|x=i}. This is then a matrig of de form  \[Rho]_{y|x} \
=Diag[\[Rho]_{y|x=1},\[Rho]_{y|x=2},\[Rho]_{y|x=3},\[Rho]_{y|x=4}]\
\>", "Text",
 CellChangeTimes->{{3.879347076534196*^9, 3.879347147446108*^9}, {
  3.879347304659616*^9, 
  3.8793473350080423`*^9}},ExpressionUUID->"6b7d846a-c00d-4cfd-9f7b-\
690b69caf031"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"\[Rho]YcondX", "[", "\[Theta]_", "]"}], ":=", 
   RowBox[{
    RowBox[{"Array", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"F", "[", 
        RowBox[{"1", ",", "##", ",", "\[Theta]"}], "]"}], "&"}], ",", "4"}], 
     "]"}], "~", "Join", "~", 
    RowBox[{"Array", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"F", "[", 
        RowBox[{"2", ",", "##", ",", "\[Theta]"}], "]"}], "&"}], ",", "4"}], 
     "]"}], "~", "Join", "~", 
    RowBox[{"Array", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"F", "[", 
        RowBox[{"3", ",", "##", ",", "\[Theta]"}], "]"}], "&"}], ",", "4"}], 
     "]"}], "~", "Join", "~", 
    RowBox[{"Array", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"F", "[", 
        RowBox[{"4", ",", "##", ",", "\[Theta]"}], "]"}], "&"}], ",", "4"}], 
     "]"}]}]}], ";"}]], "Input",
 CellChangeTimes->{{3.8793473426596622`*^9, 
  3.879347344348023*^9}},ExpressionUUID->"98d84651-6b32-405a-b3e8-\
a4d7f212d6c0"],

Cell["\<\
