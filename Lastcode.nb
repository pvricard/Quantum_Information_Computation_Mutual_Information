Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"\[Rho]Ytot", "[", "\[Theta]_", "]"}], ":=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"\[Rho]YcondX", "[", "\[Theta]", "]"}], ",", 
     RowBox[{
      RowBox[{"1", "/", "4"}], "*", 
      RowBox[{"\[Rho]Y", "[", "\[Theta]", "]"}]}]}], "}"}]}], ";"}]], "Input",
 CellLabel->
  "In[230]:=",ExpressionUUID->"a71a815a-c1d2-4643-97c3-b1d055ca7e3f"],

Cell["\<\
which will allow for computation of the different values needed at the same \
time (without needing to repeat the calculus of the function F presented \
before). Now let us move to the next step, the definition of the computation \
of the mutual information. This can be done as follows, where the eigenvalues \
are treated all at once, and just separated at the last step for the final \
computation\
\>", "Text",
 CellChangeTimes->{{3.8793478987051105`*^9, 
  3.879348070128693*^9}},ExpressionUUID->"6d430b8b-60f6-4d7f-a861-\
8639752d4b9a"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"eigen2", "[", "\[Theta]_", "]"}], ":=", 
   RowBox[{
    RowBox[{
     RowBox[{"Select", "[", 
      RowBox[{"#", ",", 
       RowBox[{
        RowBox[{"#", ">", 
         RowBox[{"10", "^", 
          RowBox[{"(", 
           RowBox[{"-", "10"}], ")"}]}]}], "&"}]}], "]"}], "&"}], "/@", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"\[Rho]Ytot", "[", "\[Theta]", "]"}], "[", 
       RowBox[{"[", "1", "]"}], "]"}], ",", 
      RowBox[{
       RowBox[{"\[Rho]Ytot", "[", "\[Theta]", "]"}], "[", 
       RowBox[{"[", "2", "]"}], "]"}]}], "}"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"totaleigen2", "[", "\[Theta]_", "]"}], ":=", 
   RowBox[{
    RowBox[{"eigen2", "[", "\[Theta]", "]"}], "*", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{"Log", "[", "#", "]"}], "&"}], "/@", 
      RowBox[{"eigen2", "[", "\[Theta]", "]"}]}], ")"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"IXY", "[", "\[Theta]_", "]"}], ":=", 
   RowBox[{
    RowBox[{"-", 
     RowBox[{"Tr", "[", 
      RowBox[{
       RowBox[{"totaleigen2", "[", "\[Theta]", "]"}], "[", 
       RowBox[{"[", "2", "]"}], "]"}], "]"}]}], "+", 
    RowBox[{
     RowBox[{"1", "/", "4"}], "*", 
     RowBox[{"Tr", "[", 
      RowBox[{
       RowBox[{"totaleigen2", "[", "\[Theta]", "]"}], "[", 
       RowBox[{"[", "1", "]"}], "]"}], "]"}]}]}]}], ";"}]}], "Input",
 CellLabel->
  "In[231]:=",ExpressionUUID->"5ca30b6a-8ad0-4572-a8df-df0420b1e322"],

